{
  "name": "flow-validator",
  "version": "0.3.7",
  "description": "object validation with flow types and more",
  "main": "lib/index.js",
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "eslint": "eslint src && eslint test",
    "flow": "flow check src && flow check test",
    "checkdeps": "nsp check",
    "test": "mocha --recursive",
    "test:file": "mocha",
    "coverage": "nyc mocha",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "codeclimate:coverage": "nyc npm test && nyc report --reporter=text-lcov | codeclimate-test-reporter",
    "coverage:ci": "nyc npm test && nyc report --reporter=text-lcov | coveralls && nyc report --reporter=text-lcov | codeclimate-test-reporter",
    "build": "rm -rf lib/* && babel src -d lib --source-maps && flow-copy-source -v src lib",
    "build:min": "uglifyjs ./lib/index.js -o ./lib/flow-validator.min.js --source-map ./lib/flow-validator.min.js.map -c -m",
    "doc:serve": "documentation serve src -g --document-exported -w -f html",
    "doc:lint": "documentation lint src -g",
    "doc:build": "documentation build src -g --document-exported -f html -o doc --sort-order alpha",
    "doc:publish": "./scripts/doc_build.sh && git push origin gh-pages:gh-pages",
    "precommit": "npm run doc:lint && npm run eslint && npm run flow && npm run checkdeps && npm test && npm run coverage && npm run build && npm run build:min",
    "prepublish": "npm run precommit",
    "postpublish": "npm run doc:publish"
  },
  "tags": [
    "flow",
    "flowtype",
    "flow",
    "flowtype",
    "js",
    "javascript",
    "schema",
    "object",
    "validation",
    "validate",
    "validator"
  ],
  "keywords": [
    "flow",
    "flowtype",
    "js",
    "javascript",
    "schema",
    "object",
    "validation",
    "validate",
    "validator"
  ],
  "author": "Frederik Batuna <gobi301@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freddi301/flow-validator.git"
  },
  "bugs": {
    "url": "https://github.com/freddi301/flow-validator/issues"
  },
  "homepage": "https://github.com/freddi301/flow-validator#readme",
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.0",
    "chai": "3.5.0",
    "codeclimate-test-reporter": "0.4.1",
    "coveralls": "2.11.16",
    "documentation": "4.0.0-beta.18",
    "eslint": "3.16.1",
    "eslint-plugin-flowtype": "2.30.0",
    "flow-bin": "0.41.0",
    "flow-copy-source": "^1.1.0",
    "husky": "^0.13.2",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "nsp": "2.6.2",
    "nyc": "10.1.2",
    "uglify-js": "^2.8.5"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0"
  }
}
